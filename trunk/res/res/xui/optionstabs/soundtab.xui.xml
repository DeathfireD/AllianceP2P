<xui>
    <body>
        <panel axis="y" id="soundtab">
            <scrollpanel border="0" scrollbars="optional">
                <panel border="0" axis="y">
                    <panel id="notification" axis="y" bordertype="etched" bordertitle="%text%">
                        <form expand="horiz" border="5">
                            <row>
                                <cell>
                                    <panel border="0" axis="y">
                                        <filler/>
                                        <label id="private" text="%text%"/>
                                    </panel>
                                </cell>
                                <cell>
                                    <panel border="0" axis="x">
                                        <filler axis="x"/>
                                        <textfield width="250" minwidth="250" maxwidth="250" id="internal.pmsound"/>
                                    </panel>
                                </cell>
                            </row>
                        </form>
                        <panel axis="x">
                            <filler/>
                            <button eventid="browsepm" text="%text%"/>
                        </panel>    
                        <form expand="horiz" border="5">
                            <row>
                                <cell>
                                    <panel border="0" axis="y">
                                        <filler/>
                                        <label id="public" text="%text%"/>
                                    </panel>
                                </cell>
                                <cell>
                                    <panel border="0" axis="x">
                                        <filler axis="x"/>
                                        <textfield width="250" minwidth="250" maxwidth="250" id="internal.publicsound"/>
                                    </panel>
                                </cell>
                            </row>
                        </form>
                        <panel axis="x">
                            <filler/>
                            <button eventid="browsepublic" text="%text%"/>
                        </panel>
                        <form expand="horiz" border="5">
                            <row>
                                <cell>
                                    <panel border="0" axis="y">
                                        <filler/>
                                        <label id="complete" text="%text%"/>
                                    </panel>
                                </cell>
                                <cell>
                                    <panel border="0" axis="x">
                                        <filler axis="x"/>
                                        <textfield width="250" minwidth="250" maxwidth="250" id="internal.downloadsound"/>
                                    </panel>
                                </cell>
                            </row>
                        </form>
                        <panel axis="x">
                            <filler/>
                            <button eventid="browsedownload" text="%text%"/>
                        </panel>
                        <strut height="10"/>
                        <mlabel id="blanknote" html="true">%text%
                        </mlabel>
                        <panel axis="x">
                            <filler/>
                            <button width="85" eventid="soundmute" text="%text%"/>
                            <strut width="5"/>
                            <button width="85" eventid="sounddefault" text="%text%"/>
                        </panel>
                    </panel>
                </panel>
            </scrollpanel>
        </panel>
    </body>
</xui>

